<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <link rel="stylesheet" href="style/index.css">
  <title>Hello Koa</title>
</head>
<body>

  <div class="list-container">
    <ul id="user-list">
      <% for(let item of list) { %>
      <li><%= item.user %> <%= item.pwd %>  <button class="delete-btn">delete</button></li>
      <% } %> 
    </ul>
  </div>
  <div class="login-container">
    <form id="user-log-form" >
      <label for="user-name">User Name:</label><br>
      <input type="text" id="user-name" name="user" maxlength="10"><br>
      <label for="password">Password:</label><br>
      <input type="password" id="password" name="pwd" maxlength="15" size="7"><br>
      <button id="loging-btn" type="button">Sign up</button>
    </form>
  </div>
  <script src="script/main.js"></script>
</body>
</html>
